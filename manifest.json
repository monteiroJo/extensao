{
    "name": "removeer",
    "description": "Hides YouTube Shorts and the progress bar on YouTube thumbnails.",
    "version": "0.3",
    "manifest_version": 3,
    "action": {
      "default_icon": "hi.png"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.youtube.com/*",
          "http://*.youtube.com/watch?*",
          "https://*.youtube.com/watch?*"],
        "css": ["removo.css"]
      }
    ]
  }